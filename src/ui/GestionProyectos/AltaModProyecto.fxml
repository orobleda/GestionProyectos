<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="anchor" fx:id="anchor" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.GestionProyectos.AltaModProyecto">
   <children>
      <Accordion prefHeight="600.0">
        <panes>
          <TitledPane fx:id="tpFiltros" text="Modificar Elemento">
               <content>
                  <StackPane prefWidth="200.0">
                     <children>
                        <VBox>
                           <children>
                              <HBox>
                                 <children>
                                    <Label text="Elemento">
                                       <opaqueInsets>
                                          <Insets />
                                       </opaqueInsets>
                                       <HBox.margin>
                                          <Insets left="15.0" right="50.0" />
                                       </HBox.margin>
                                       <padding>
                                          <Insets left="15.0" right="15.0" />
                                       </padding>
                                    </Label>
                                    <ComboBox fx:id="cbListaProy" prefWidth="450.0">
                                       <opaqueInsets>
                                          <Insets left="15.0" />
                                       </opaqueInsets>
                                       <padding>
                                          <Insets left="15.0" />
                                       </padding>
                                    </ComboBox>
                                 </children>
                                 <padding>
                                    <Insets bottom="15.0" />
                                 </padding>
                              </HBox>
                              <HBox>
                                 <children>
                                    <ImageView fx:id="imAniadir" accessibleText="Añadir" fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                                       <image>
                                          <Image url="@../../../imgs/iconos/aniadir.png" />
                                       </image>
                                       <HBox.margin>
                                          <Insets right="25.0" />
                                       </HBox.margin>
                                       <cursor>
                                          <Cursor fx:constant="HAND" />
                                       </cursor>
                                    </ImageView>
                                    <ImageView fx:id="imBuscar" fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                                       <image>
                                          <Image url="@../../../imgs/iconos/buscar.png" />
                                       </image>
                                       <cursor>
                                          <Cursor fx:constant="HAND" />
                                       </cursor>
                                    </ImageView>
                                 </children>
                                 <VBox.margin>
                                    <Insets bottom="25.0" left="650.0" />
                                 </VBox.margin>
                              </HBox>
                           </children>
                        </VBox>
                        
                     </children>
                  </StackPane>
               </content>
          </TitledPane>
          <TitledPane fx:id="tpValores" text="Valores">
               <content>
                  <StackPane>
                     <children>
                        <VBox>
                           <children>
                              <HBox prefWidth="200.0">
                                 <children>
                                    <Label text="Id">
                                       <HBox.margin>
                                          <Insets left="15.0" right="50.0" />
                                       </HBox.margin>
                                       <padding>
                                          <Insets right="38.0" />
                                       </padding>
                                    </Label>
                                    <TextField fx:id="tID" editable="false" />
                                 </children>
                              </HBox>
                              <HBox prefHeight="0.0" prefWidth="200.0">
                                 <children>
                                    <Label text="Nombre">
                                       <HBox.margin>
                                          <Insets left="15.0" right="55.0" />
                                       </HBox.margin>
                                    </Label>
                                    <TextField fx:id="tNombreProy" maxWidth="700.0" prefWidth="600.0" />
                                 </children>
                                 <VBox.margin>
                                    <Insets bottom="10.0" top="15.0" />
                                 </VBox.margin>
                              </HBox>
                              <HBox prefHeight="100.0" prefWidth="200.0">
                                 <children>
                                    <VBox>
                                       <children>
                                          <TitledPane animated="false" prefHeight="500.0" text="Parámetros">
                                             <content>
                                                <TableView fx:id="tParametros" minHeight="300.0" prefHeight="500.0">
                                                  <columns>
                                                    <TableColumn fx:id="cTipo" prefWidth="75.0" text="Tipo" />
                                                    <TableColumn fx:id="cCodigo" prefWidth="88.0" text="Código" />
                                                      <TableColumn fx:id="cDescrip" prefWidth="326.0" text="Descripción" />
                                                      <TableColumn fx:id="cValor" minWidth="0.0" prefWidth="186.0" text="Valor" />
                                                  </columns>
                                                   <columnResizePolicy>
                                                      <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                                   </columnResizePolicy>
                                                </TableView>
                                             </content>
                                             <VBox.margin>
                                                <Insets left="15.0" />
                                             </VBox.margin>
                                          </TitledPane>
                                       </children>
                                       <HBox.margin>
                                          <Insets top="25.0" />
                                       </HBox.margin>
                                    </VBox>
                                 </children>
                              </HBox>
                              <HBox>
                                 <children>
                                    <ImageView fx:id="imGuardar" accessibleText="Añadir" fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true" styleClass="iconoDisabled">
                                       <image>
                                          <Image url="@../../../imgs/iconos/guardar.jpg" />
                                       </image>
                                       <HBox.margin>
                                          <Insets left="630.0" right="25.0" />
                                       </HBox.margin>
                                    </ImageView>
                                    <ImageView fx:id="imEliminar" accessibleText="Añadir" fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true" styleClass="iconoDisabled">
                                       <image>
                                          <Image url="@../../../imgs/iconos/eliminar.png" />
                                       </image>
                                    </ImageView>
                                 </children>
                                 <VBox.margin>
                                    <Insets top="45.0" />
                                 </VBox.margin>
                              </HBox>
                           </children>
                        </VBox>
                     </children>
                  </StackPane>
               </content>
          </TitledPane>
        </panes>
         <padding>
            <Insets left="10.0" right="10.0" top="7.0" />
         </padding>
      </Accordion>
   </children>
</AnchorPane>
