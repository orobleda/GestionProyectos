<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.Administracion.Parametricas.AdministracionParametros">
   <center>
      <VBox>
         <children>
            <HBox fx:id="hbCentro" alignment="TOP_CENTER" BorderPane.alignment="CENTER">
               <VBox.margin>
                  <Insets bottom="25.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="TOP_RIGHT">
               <children>
                  <ImageView fx:id="imGuardar" fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../../imgs/iconos/Guardar3_PAS.png" />
                     </image>
                  </ImageView>
               </children>
               <VBox.margin>
                  <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </center>
   <top>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="BASELINE_CENTER">
               <children>
                  <Label prefWidth="200.0" text="Entidad">
                     <padding>
                        <Insets right="15.0" />
                     </padding>
                  </Label>
                  <ComboBox fx:id="cbElemento" prefWidth="300.0" />
               </children>
            </HBox>
            <HBox alignment="BASELINE_CENTER">
               <children>
                  <Label prefWidth="200.0" text="Elemento">
                     <padding>
                        <Insets right="15.0" />
                     </padding>
                  </Label>
                  <ComboBox fx:id="cbidElemento" prefWidth="300.0" />
               </children>
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
            </HBox>
         </children>
         <BorderPane.margin>
            <Insets bottom="20.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
      </VBox>
   </top>
</BorderPane>
