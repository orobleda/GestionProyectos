<BorderPane prefHeight="-1.0" prefWidth="-1.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.Economico.EstimacionesInternas.EstimacionesInternas">
  <bottom>
    <VBox alignment="TOP_CENTER" prefHeight="-1.0" prefWidth="-1.0" BorderPane.margin="$x1">
      <children>
        <HBox alignment="TOP_RIGHT" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
          <children>
            <Label text="Mostrar sólo los proyectos con un resto mayor o igual  que " />
            <TextField fx:id="tFiltroResumen" prefWidth="75.0">
              <HBox.margin>
                <Insets left="20.0" fx:id="x2" />
              </HBox.margin>
