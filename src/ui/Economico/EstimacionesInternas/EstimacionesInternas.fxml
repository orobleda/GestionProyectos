                  <ImageView id="imGuardar" fx:id="imAniadir" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../../imgs/iconos/Nuevo3_PAS.png" />
                     </image>
                     <HBox.margin>
                        <Insets left="46.0" />
                     </HBox.margin>
                  </ImageView>
               </children>
        </HBox>
      </children>
    </VBox>
