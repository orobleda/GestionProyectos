         <BorderPane.margin>
            <Insets left="10.0" />
         </BorderPane.margin>
      </ScrollPane>
  </left>
  <top>
    <VBox prefHeight="-1.0" prefWidth="-1.0" BorderPane.alignment="TOP_CENTER">
