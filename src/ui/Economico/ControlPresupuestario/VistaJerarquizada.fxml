                        <Image url="@../../../../imgs/iconos/antPagina_PAS.png" />
                     </image>
                  </ImageView>
                  <ComboBox fx:id="cbMeses" prefWidth="250.0">
                     <HBox.margin>
                        <Insets left="50.0" right="50.0" />
                     </HBox.margin>
                  </ComboBox>
                  <ImageView fx:id="iMSigPag" fitHeight="150.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../../imgs/iconos/sigPagina_PAS.png" />
                     </image>
                  </ImageView>
               </children>
            </HBox>
            <TabPane fx:id="tbPane" tabClosingPolicy="UNAVAILABLE">
