      </HBox>
      <HBox alignment="TOP_RIGHT">
         <children>
            <ImageView fx:id="imGuardar" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../../imgs/iconos/Guardar3_PAS.png" />
