                        <Image url="@../../../../imgs/iconos/Analizar3_PAS.png" />
                     </image>
                     <HBox.margin>
                        <Insets right="15.0" />
                     </HBox.margin>
                  </ImageView>
               </children>
            </HBox>
            <HBox>
               <VBox.margin>
                  <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
               </VBox.margin>
               <children>
                  <Label prefWidth="150.0" text="Proyectos Encontrados" />
                  <ComboBox fx:id="cbProyectos" prefWidth="350.0" />
