<?xml version="1.0" encoding="UTF-8"?>

<?import org.controlsfx.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.Economico.CargaImputaciones.DetalleImputacion">
   <center>
      <HBox alignment="CENTER_LEFT" BorderPane.alignment="CENTER">
         <children>
            <ImageView id="imGuardar" fx:id="imSemaforo" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../imgs/iconos/AltaUsuario3_PAS.png" />
               </image>
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </ImageView>
            <TableView fx:id="tResultado" />
            <ComboBox fx:id="cbSistema" prefWidth="200.0">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
            </ComboBox>
            <ImageView id="imGuardar" fx:id="imAltaUsuario" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../imgs/iconos/AltaUsuario3_PAS.png" />
               </image>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </ImageView>
            <ImageView id="imGuardar" fx:id="imAltaTarifa" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../imgs/iconos/AltaTarifa3_PAS.png" />
               </image>
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </ImageView>
            <Label fx:id="lAccionEstimacion" text="Incluir Dato">
               <HBox.margin>
                  <Insets left="15.0" />
               </HBox.margin>
            </Label>
            <ToggleSwitch fx:id="tsAccionEstimacion" />
         </children>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
      </HBox>
   </center>
</BorderPane>
