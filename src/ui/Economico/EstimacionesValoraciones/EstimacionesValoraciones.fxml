<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="anchor" prefHeight="700.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.Economico.EstimacionesValoraciones.EstimacionesValoraciones">
   <children>
      <Accordion AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <panes>
          <TitledPane fx:id="tpConsulta" animated="false" text="Consulta">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0">
                     <children>
                        <VBox AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
                           <children>
                              <HBox>
                                 <children>
                                    <Label layoutX="1.0" layoutY="1.0" prefHeight="17.0" prefWidth="175.0" text="Proyecto" />
                                    <ComboBox fx:id="cbProyectos" layoutX="2.0" layoutY="1.0" prefWidth="400.0" />
                                 </children>
                              </HBox>
                              <HBox>
                                 <children>
                                    <Label layoutX="1.0" layoutY="2.0" prefWidth="175.0" text="Versión presupuesto" />
                                    <ComboBox fx:id="cbVsPresupuesto" layoutX="2.0" layoutY="1.0" prefWidth="400.0">
                                       <HBox.margin>
                                          <Insets />
                                       </HBox.margin>
                                    </ComboBox>
                                 </children>
                                 <VBox.margin>
                                    <Insets top="20.0" />
                                 </VBox.margin>
                              </HBox>
                              <HBox alignment="TOP_RIGHT">
                                 <children>
                                    <ImageView fx:id="imAniadirPresupuesto" fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                                       <image>
                                          <Image url="@../../../../imgs/iconos/aniadir.png" />
                                       </image>
                                       <cursor>
                                          <Cursor fx:constant="HAND" />
                                       </cursor>
                                    </ImageView>
                                    <ImageView fx:id="imBuscarPresupuesto" fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                                       <image>
                                          <Image url="@../../../../imgs/iconos/buscar.png" />
                                       </image>
                                       <cursor>
                                          <Cursor fx:constant="HAND" />
                                       </cursor>
                                       <HBox.margin>
                                          <Insets left="20.0" />
                                       </HBox.margin>
                                    </ImageView>
                                 </children>
                                 <VBox.margin>
                                    <Insets bottom="20.0" top="20.0" />
                                 </VBox.margin>
                              </HBox>
                           </children>
                        </VBox>
                     </children>
                  </AnchorPane>
            </content>
          </TitledPane>
          <TitledPane fx:id="tpResultados" animated="false" text="Estimaciones &amp; Valoraciones">
            <content>
              <AnchorPane fx:id="aResultados" minHeight="0.0" minWidth="0.0">
                     <children>
                        <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                           <children>
                              <HBox>
                                 <children>
                                    <Label prefWidth="175.0" text="Id" />
                                    <TextField fx:id="tId" disable="true" editable="false" prefWidth="75.0" />
                                    <Label prefWidth="100.0" text="Fecha Alta">
                                       <HBox.margin>
                                          <Insets left="25.0" />
                                       </HBox.margin>
                                    </Label>
                                    <TextField fx:id="tFxAlta" disable="true" editable="false" />
                                 </children>
                              </HBox>
                              <HBox>
                                 <children>
                                    <Label prefWidth="175.0" text="Versión" />
                                    <TextField fx:id="tVersion" disable="true" editable="false" prefWidth="75.0" />
                                 </children>
                                 <VBox.margin>
                                    <Insets top="10.0" />
                                 </VBox.margin>
                              </HBox>
                              <HBox prefHeight="25.0" prefWidth="200.0">
                                 <VBox.margin>
                                    <Insets top="25.0" />
                                 </VBox.margin>
                                 <children>
                                    <Label prefWidth="175.0" text="Descripción Presupuesto" />
                                    <TextArea fx:id="taDesc" disable="true" editable="false" prefWidth="300.0" />
                                    <ImageView fx:id="imEditDesc" fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true" styleClass="iconoDisabled">
                                       <image>
                                          <Image url="@../../../../imgs/iconos/edit.png" />
                                       </image>
                                       <HBox.margin>
                                          <Insets left="25.0" />
                                       </HBox.margin>
                                    </ImageView>
                                 </children>
                              </HBox>
                              <HBox>
                                 <children>
                                    <Label prefWidth="175.0" text="Tipo Presupuesto" />
                                    <ComboBox fx:id="cbTipoPrep" disable="true" prefWidth="300.0" />
                                    <ImageView fx:id="imEditTipoPres" fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true" styleClass="iconoDisabled">
                                       <image>
                                          <Image url="@../../../../imgs/iconos/edit.png" />
                                       </image>
                                       <HBox.margin>
                                          <Insets left="25.0" />
                                       </HBox.margin>
                                    </ImageView>
                                 </children>
                                 <VBox.margin>
                                    <Insets top="25.0" />
                                 </VBox.margin>
                              </HBox>
                              <HBox alignment="TOP_RIGHT">
                                 <children>
                                    <ImageView fx:id="imAniadirSistma" fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true" styleClass="iconoDisabled">
                                       <image>
                                          <Image url="@../../../../imgs/iconos/aniadir.png" />
                                       </image>
                                       <HBox.margin>
                                          <Insets left="20.0" />
                                       </HBox.margin>
                                    </ImageView>
                                 </children>
                                 <VBox.margin>
                                    <Insets bottom="15.0" top="25.0" />
                                 </VBox.margin>
                                 <padding>
                                    <Insets right="25.0" />
                                 </padding>
                              </HBox>
                              <HBox alignment="TOP_CENTER">
                                 <children>
                                    <TableView fx:id="tLineasCoste" prefHeight="550.0" prefWidth="1000.0" />
                                 </children>
                              </HBox>
                              <HBox alignment="TOP_CENTER">
                                 <children>
                                    <TableView fx:id="tResumenCoste" prefWidth="1000.0" />
                                 </children>
                                 <VBox.margin>
                                    <Insets bottom="15.0" top="15.0" />
                                 </VBox.margin>
                              </HBox>
                              <HBox alignment="TOP_RIGHT">
                                 <children>
                                    <ImageView fx:id="imVersionarPres" fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true" styleClass="iconoDisabled">
                                       <image>
                                          <Image url="@../../../../imgs/iconos/nuevaVersion.png" />
                                       </image>
                                    </ImageView>
                                    <ImageView fx:id="imGuardarPresupuesto" fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true" styleClass="iconoDisabled">
                                       <image>
                                          <Image url="@../../../../imgs/iconos/guardar.jpg" />
                                       </image>
                                       <HBox.margin>
                                          <Insets left="20.0" right="25.0" />
                                       </HBox.margin>
                                    </ImageView>
                                    <ImageView fx:id="imEliminarPres" fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true" styleClass="iconoDisabled">
                                       <image>
                                          <Image url="@../../../../imgs/iconos/eliminar.png" />
                                       </image>
                                    </ImageView>
                                 </children>
                                 <VBox.margin>
                                    <Insets top="25.0" />
                                 </VBox.margin>
                              </HBox>
                           </children>
                        </VBox>
                     </children>
                  </AnchorPane>
            </content>
          </TitledPane>
        </panes>
      </Accordion>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</AnchorPane>
